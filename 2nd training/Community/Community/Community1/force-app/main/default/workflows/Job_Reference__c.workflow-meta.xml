    <rules>
        <fullName>TMS - DTD</fullName>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND (4 OR 5)</booleanFilter>
        <criteriaItems>
            <field>Job_Reference__c.DTD_DTP__c</field>
            <operation>equals</operation>
            <value>DTD</value>
        </criteriaItems>
        <criteriaItems>
            <field>Job_Reference__c.ATA_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Job_Reference__c.RecordTypeId</field>
            <operation>equals</operation>
            <value>HK Export,HK Import,HK Import &amp; Export</value>
        </criteriaItems>
        <criteriaItems>
            <field>Job_Reference__c.Days_in_Transit_Storage__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Job_Reference__c.Delivery_Date__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>For Door to Door shipment, we must fill in “Days in Transit Storage” and “Delivery Date” within 7 days of “ATA” date,</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>TMS_DTD_Update_Transit_Delivery_Dates</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Job_Reference__c.ATA_Date__c</offsetFromField>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
    <rules>
        <fullName>TMS - DTP</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Job_Reference__c.DTD_DTP__c</field>
            <operation>equals</operation>
