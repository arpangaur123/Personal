        </criteriaItems>
        <criteriaItems>
            <field>Job_Reference__c.Custom_Clearance__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <description>For Door to Port shipment, we must fill in “Custom Clearance Date” within 2 days of “ATA” date,</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
