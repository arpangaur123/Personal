    <rules>
        <fullName>Alert 35 days before permit expiry date %28BJ%29</fullName>
        <active>true</active>
        <criteriaItems>
            <field>Job_Reference__c.Permit_Expiry_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>User.ProfileId</field>
            <operation>contains</operation>
            <value>AMBJ</value>
        </criteriaItems>
        <description>35 days before permit expiry date, alert will be sent out</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Alert_for_Permit_Expiring_within_35_days_BJ</name>
                <type>Alert</type>
            </actions>
            <offsetFromField>Job_Reference__c.Permit_Expiry_Date__c</offsetFromField>
            <timeLength>-35</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
