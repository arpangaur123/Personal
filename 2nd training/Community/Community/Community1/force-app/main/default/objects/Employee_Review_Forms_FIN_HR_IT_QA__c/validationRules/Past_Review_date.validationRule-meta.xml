<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Past_Review_date</fullName>
    <active>true</active>
    <description>Next Review Period cannot be past date.</description>
    <errorConditionFormula>Next_Review_Period__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Next_Review_Period__c</errorDisplayField>
    <errorMessage>The next Review Period cannot be past the date.</errorMessage>
</ValidationRule>
