    <url>trigger accountNameCheck on Account(before insert) {
 	if (Trigger.new.Name != NULL)
 	{
 		Account[] accounts = [select id from account where name = :Trigger.new.Name];
 		if (Accounts.size() &gt; 0) {
 			Trigger.new.Name.addError(&apos;Account name already exists&apos;);
 		}
 		
 	}
}</url>
</WebLink>
